<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	<meta name="description" content="">
	<meta name="author" content="">
	<title>@ViewBag.Title</title>
	@Styles.Render("~/Content/Admin/css")
	@Scripts.Render("~/bundles/Admin/modernizr")
	<link rel="stylesheet" type="text/css" href="~/Content/AdminTheme/vendor/font-awesome/css/font-awesome.min.css" />
</head>
<body id="wrapper" class="body-content">
	<div id="wrapper">
		<!-- Navigation -->
		@Html.Partial("_Nav")
		<div id="page-wrapper">
			<div class="row">
				<div class="col-lg-12">
					@if (IsSectionDefined("header"))
					{
						@RenderSection("header", required: true)
					}
					else
					{
						<h1 class="page-header">Admin</h1>
					}
				</div>
				<!-- /.col-lg-12 -->
			</div>
			<!-- /.row -->
			<div class="row">
				@RenderBody()
				<!-- /.col-lg-12 -->
			</div>
			<!-- /.row -->

		</div>
		<!-- /#page-wrapper -->
	</div>
	<!-- jQuery -->
	@Scripts.Render("~/bundles/Admin/jquery")
	@Scripts.Render("~/bundles/Admin/bootstrap")
	<script>
		$(document).ready(function () {
			$('#dataTables-example').DataTable({
				responsive: true
			});
		});
	</script>
	@RenderSection("scripts", required: false)
</body>
</html>